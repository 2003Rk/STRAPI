{
  "name": "strapi-railway-deploy",
  "version": "1.0.0",
  "description": "Strapi backend with webhook server for Railway deployment",
  "main": "webhook-server.js",
  "scripts": {
    "build": "cd strapi-backend && npm install && npm run build",
    "start": "cd strapi-backend && npm run start",
    "develop": "cd strapi-backend && npm run develop",
